{
    "name": "tpmanc/cms",
    "description": "Yii 2 CMS",
    "keywords": ["yii2", "framework", "cms"],
    "homepage": "https://github.com/tpmanc/cms",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/tpmanc/cms/issues",
        "source": "https://github.com/tpmanc/cms"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "2.0.5",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "yiidoc/yii2-redactor": "*",
        "creocoder/yii2-nested-sets": "0.9.*",
        "tpmanc/cms-core": "*",
        "tpmanc/yii2-file-behavior": "*",
        "tpmanc/yii2-sitemap": "*"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
